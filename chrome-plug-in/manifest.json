{
  "name": "chrome plug-in name",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ],
  "browser_action": {
    "default_icon": "action_icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "action_icon.png",
    "32": "action_icon.png",
    "128": "action_icon.png"
  }
}